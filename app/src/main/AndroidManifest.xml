<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jake.fantasy">

    <uses-permission android:name="android.name.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/shakib"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignInActivity" />
        <activity
            android:name=".TabbedActiviy"
            android:label="@string/title_activity_tabbed_activiy"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".WelcomeActiviy" />
        <activity android:name=".CreateAccountActivity" />
        <activity android:name=".CreatingTeam1" />
        <activity android:name=".PlayerList" />
        <activity android:name=".CreatingTeam2" />
        <activity android:name=".FilterActivity" />
        <activity android:name=".PlayerStat" />
        <activity android:name=".TeamStats" />

        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".MyFirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <activity android:name=".OtherTeams" />
        <activity android:name=".RulesActivity"></activity>
    </application>

</manifest>